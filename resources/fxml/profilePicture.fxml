<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchorPane" prefHeight="289.0" prefWidth="559.0"
            xmlns="http://javafx.com/javafx/8.0.192"
            fx:controller="SuperSecureChat.Controller.ProfilePictureController">
    <children>
        <Canvas fx:id="canvas" height="256.0" layoutX="14.0" layoutY="14.0" width="256.0" AnchorPane.leftAnchor="14.0"
                AnchorPane.topAnchor="14.0"/>
        <JFXButton buttonType="RAISED" layoutX="300.0" layoutY="130.0" mnemonicParsing="false"
                   onMouseClicked="#openFileDialog" styleClass="fileButton" text="Durchsuchen..."/>
        <JFXButton buttonType="RAISED" layoutX="300.0" layoutY="188.0" mnemonicParsing="false"
                   onMouseClicked="#saveProfilePicture" styleClass="saveButton" text="Speichern"/>
    </children>
</AnchorPane>
