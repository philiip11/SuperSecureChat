<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXBadge?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<AnchorPane fx:id="anchorPane" prefHeight="40.0" styleClass="contactItem" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <StackPane>
            <JFXBadge fx:id="badge" position="BOTTOM_RIGHT" styleClass="text-badge">
                <ImageView fx:id="contactImage" fitHeight="48.0" fitWidth="48.0" pickOnBounds="true"
                           preserveRatio="true"/>
            </JFXBadge>
        </StackPane>

        <Label fx:id="label1" layoutX="50.0" layoutY="16.0" text="Label"/>
    </children>
</AnchorPane>
